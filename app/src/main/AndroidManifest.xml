<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="assignment.zunzelf.org.pra">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/histogram_ic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.SkeletonRecognition" />
        <activity android:name=".view.Histogram" />
        <activity android:name=".view.EnT1" />
        <activity android:name=".view.EnT2" />
        <activity android:name=".view.ArialDigit" />
        <activity android:name=".view.SevenSegment" />
        <activity android:name=".view.Thinning" />
        <activity android:name=".view.DataSetManagement" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".view.OCR" />
        <activity android:name=".view.FaceRegion" />
        <activity android:name=".view.Fourier"></activity>
    </application>

</manifest>